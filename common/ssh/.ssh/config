Host github.com
    IdentityFile ~/.ssh/github
    IdentitiesOnly yes

Host bitbucket.org
    IdentityFile ~/.ssh/bitbucket
    IdentitiesOnly yes

Host gitlab.com
    IdentityFile ~/.ssh/gitlab
    IdentitiesOnly yes

Host 192.168.*.*
    IdentityFile ~/.ssh/internal
    IdentitiesOnly yes

Host *.compute.amazonaws.com
    IdentityFile ~/.ssh/aws
    IdentitiesOnly yes
    User ec2-user
