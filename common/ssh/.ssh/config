Include config.d/*

Host github.com
    IdentityFile ~/.ssh/github
    IdentitiesOnly yes
    UserKnownHostsFile ~/.ssh/known_vcs_hosts

Host bitbucket.org
    IdentityFile ~/.ssh/bitbucket
    IdentitiesOnly yes
    UserKnownHostsFile ~/.ssh/known_vcs_hosts

Host gitlab.com
    IdentityFile ~/.ssh/gitlab
    IdentitiesOnly yes
    UserKnownHostsFile ~/.ssh/known_vcs_hosts

Host ssh.dev.azure.com
    IdentityFile ~/.ssh/azure-dev
    IdentitiesOnly yes
    UserKnownHostsFile ~/.ssh/known_vcs_hosts

Host 192.168.*.*
    IdentityFile ~/.ssh/internal
    IdentitiesOnly yes
    UserKnownHostsFile ~/.ssh/known_internal_hosts

Host *.compute.amazonaws.com
    IdentityFile ~/.ssh/aws
    IdentitiesOnly yes
    UserKnownHostsFile ~/.ssh/known_aws_hosts
    User ec2-user
